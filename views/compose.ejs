<%- include("partials/header"); -%>

<h1>Compose</h1>

<form class="publish" action="/compose" method="post">
  <div class="form-group">
    <label class="form-label">Title</label><br>
    <input class="form-control" type="text" name="postTitle"><br>
    <label class="form-label">Post</label><br>
    <textarea class="form-control" name="postContent" rows="8" cols="80"></textarea><br>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>



<%- include("partials/footer"); -%>
